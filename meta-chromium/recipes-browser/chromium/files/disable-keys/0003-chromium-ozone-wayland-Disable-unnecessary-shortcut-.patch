From 3b513868f4364d05c4f17ef26aaabeb056efe4a9 Mon Sep 17 00:00:00 2001
From: Hara Tomoyuki <tomoyuki.hara@miraclelinux.biz>
Date: Fri, 11 Nov 2022 13:22:59 +0900
Subject: [PATCH 03/10] chromium-ozone-wayland: Disable unnecessary shortcut
 keys.(FCNCHMI-224)

---
 third_party/blink/common/input/web_mouse_wheel_event.cc | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/third_party/blink/common/input/web_mouse_wheel_event.cc b/third_party/blink/common/input/web_mouse_wheel_event.cc
index a7f5411a38d3d..436ae80b38ee7 100644
--- a/third_party/blink/common/input/web_mouse_wheel_event.cc
+++ b/third_party/blink/common/input/web_mouse_wheel_event.cc
@@ -119,7 +119,7 @@ WebMouseWheelEvent::GetPlatformSpecificDefaultEventAction(
   // don't trigger scrolling. Instead, they may cause zooming.
   if (event.delta_units != ui::ScrollGranularity::kScrollByPrecisePixel &&
       (event.GetModifiers() & WebInputEvent::kControlKey)) {
-    return blink::WebMouseWheelEvent::EventAction::kPageZoom;
+    return blink::WebMouseWheelEvent::EventAction::kScroll;
   }
 
   if (event.delta_x == 0 && (event.GetModifiers() & WebInputEvent::kShiftKey))
